<div style=" margin-top: 1%;margin-bottom: 1%;margin-left: 1%;margin-right: 1%;"> 
    <h1 class="text-center">Sign In</h1>

    <%= form_with url: sessions_path do |f| %>
        <div class="p-1">
            <%= f.label :email%>
            <%= f.email_field :email%> 
        </div>
        <div class="p-1">
            <%= f.label :password%>
            <%= f.password_field :password%> 
        </div>
        <%= f.submit "Sign In" %>
    <% end %>
</div>
